{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"DOM",
			"ESNext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"incremental": true,
		"baseUrl": ".",
	},
	"exclude": [
		"dist",
		"node_modules",
		"wordpress"
	]
}
